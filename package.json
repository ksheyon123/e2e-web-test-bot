{
  "name": "express-puppeteer-automation",
  "version": "1.0.0",
  "description": "Express server with Puppeteer automation",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon --watch \"**/*.ts\" --exec \"ts-node\" app.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.13",
    "@langchain/core": "^0.3.39",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "langchain": "^0.3.15",
    "morgan": "^1.10.0",
    "proxy-agent": "^6.5.0",
    "puppeteer": "^24.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.1",
    "@types/puppeteer": "^7.0.4",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
